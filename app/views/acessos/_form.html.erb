<%= form_for @acesso, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <%= f.collection_select(:user_id,User.order(:email),:id,:email,{},{ :class => 'form-control'}) %>
  </div>
  <div class="form-group">
    <%= f.label :menusub_id, :class => 'control-label' %>
    <%= f.collection_select( :menusub_id,Menusub.order(:descricao),:id,:descricao,{}, {:class => 'form-control'}) %>
  </div>
  <div class="form-group">
    <%= f.label :acao, :class => 'control-label' %>
    <%= f.select :acao,{"Leitura"=>"read","Alteração"=>"update","Criação"=>"create","Total"=>"manage"},{}, :class => 'form-control' %>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                acessos_path, :class => 'btn' %>
  </div>
<% end %>
