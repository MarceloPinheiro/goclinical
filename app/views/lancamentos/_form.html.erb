<%= form_for @lancamento, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :obra, :class => 'control-label' %>
    <%= f.collection_select(:obra,Obra.order(:obra),:id,:obra,{:prompt=>false},{:class => 'form-control'}) %>
  </div>
  <div class="form-group">
    <%= f.label :fornecedor, :class => 'control-label' %>
    <%= f.collection_select(:fornecedor,Fornecedore.order(:fornecedor),:id,:fornecedor,{:prompt=>false},{:class=>'form-control'}) %>
  </div>
  <div class="form-group">
    <%= f.label :nota, :class => 'control-label' %>
    <%= f.number_field :nota, :class => 'form-control' %>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                lancamentos_path, :class => 'btn' %>
  </div>
<% end %>
